{
    "compilerOptions": {
        "incremental": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "declaration": true,
        "stripInternal": true,
        "noEmitOnError": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    }
}
